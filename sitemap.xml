---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>1.0</priority>
  </url>
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/news/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.8</priority>
  </url>
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/bio/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.8</priority>
  </url>
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/music/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.9</priority>
  </url>
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/shows/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.8</priority>
  </url>
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/photos/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.7</priority>
  </url>
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/video/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.7</priority>
  </url>
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/friends/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.5</priority>
  </url>
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/contact/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.6</priority>
  </url>
  {% for post in site.posts %}
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}{{ post.url }}</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <priority>0.6</priority>
  </url>
  {% endfor %}
  {% for member in site.data.bio.members %}
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/bio/{{ member.slug }}/</loc>
    <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
    <priority>0.5</priority>
  </url>
  {% endfor %}
</urlset>
